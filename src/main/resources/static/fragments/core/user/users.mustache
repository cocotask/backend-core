<table>
    <thead>
        <tr>
            <th>이름</th>
            <th>이메일</th>
            <th>수정/삭제</th>
        </tr>
    </thead>
    <tbody>
        {{#users}}
        <tr>
            <td><a href="#" class="_readUserBtn" data-user-uid="{{uid}}">{{userName}}</a></td>
            <td>{{userEmail}}</td>
            <td>
                <a href="#" class="_updateUserBtn" data-user-uid="{{uid}}">수정</a>
                <a href="#" class="_deleteUserBtn" data-user-uid="{{uid}}">삭제</a>
            </td>
        </tr>
        {{/users}}
    </tbody>
</table>

